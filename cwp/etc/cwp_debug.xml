<?xml version="1.0" encoding="UTF-8"?>
<root>
    <aftn_message_processor>
        <FPL2012>false</FPL2012>
        <cut_fir>true</cut_fir>
        <empty18field>false</empty18field>
        <fir_name>UTTR,UTT2</fir_name>
        <validate_route>true</validate_route>
    </aftn_message_processor>
    <airplane>
        <FL_SI>FM<!--F only feet, M only meters, FM or MF feet and meters with buttons-->
        </FL_SI>
        <MTCD_enable>false</MTCD_enable>
        <aproximation>
            <fatal_interval>30</fatal_interval>
            <interval>30</interval>
            <reminder_interval>4</reminder_interval>
        </aproximation>
        <auto_collapse>no</auto_collapse>
        <deflexion_kilometer>20</deflexion_kilometer>
        <deflexion_marks>true</deflexion_marks>
        <deflexion_sec>3</deflexion_sec>
        <deflexion_type>kilometer</deflexion_type>
        <high_FL>500</high_FL>
        <history_number_of_points>5</history_number_of_points>
        <low_FL>0</low_FL>
        <size_plane_point>3</size_plane_point>
        <turn_time>
            <backward>240</backward>
            <forward>0</forward>
            <side>120</side>
        </turn_time>
    </airplane>
    <databases timeout="6">
        <database1>
            <driver>QPSQL</driver>
            <host>192.20.1.45</host>
            <name>aero</name>
            <password>aero</password>
            <port>5432</port>
            <user>aero</user>
        </database1>
        <database2>
            <driver>QOCI</driver>
        <host>192.20.1.45</host>
            <name>XE</name>
            <password>aero</password>
            <port>1521</port>
            <user>aero</user>
        </database2>
    </databases>
    <fpl>
        <clear_FPL_on_start>no</clear_FPL_on_start>
    </fpl>
    <hotkeys>
        <ExpandList>Ctrl+E</ExpandList>
        <RestoreLabels>Ctrl+Z</RestoreLabels>
        <RunwayWidgetMinimize>Esc</RunwayWidgetMinimize>
        <ORCAMDialog>Ctrl+S</ORCAMDialog>
    </hotkeys>
    <log>
        <file_size>1048576</file_size>
        <level>DEBUG</level>
        <max_backup_index>10</max_backup_index>
        <path>cwp.log</path>
        <operations_log_path>operations.log</operations_log_path>
    </log>
    <meteo>
        <airport>UMMS</airport>
        <editable>true</editable>
        <table_only>false</table_only>
    </meteo>
    <oldi>
        <cwp_name>CWPB,CWPC,CWPD</cwp_name>
        <disableCOF>false</disableCOF>
        <disable_COF>false</disable_COF>
        <disable_HOP>false</disable_HOP>
        <disable_ROF>false</disable_ROF>
        <double_code_re_act>false</double_code_re_act>
        <fdp_name>FDP,FDPR</fdp_name>
        <lam_timeout>10</lam_timeout>
        <owner_name>CWPA</owner_name>
        <time_act>600</time_act>
        <time_cof>60</time_cof>
        <transfer_show_time>60</transfer_show_time>
        <transfer_timeout>60</transfer_timeout>
    </oldi>
    <reminders>
        <FL_dev>3</FL_dev>
        <FL_timeout>5</FL_timeout>
        <auto_transfer_out_minutes>10</auto_transfer_out_minutes>
        <default_VRC>30</default_VRC>
        <leave_route_meters>10000</leave_route_meters>
    </reminders>
    <simulator>
        <asterix>
            <listener>
                <ip>0.0.0.0</ip>
                <port>4035</port>
            </listener>
            <listener_uvd>
                <ip>0.0.0.0</ip>
                <port>30000</port>
                <use_new_parser>true</use_new_parser>
            </listener_uvd>
        </asterix>
        <bearing>
            <bearing_show_time>10</bearing_show_time>
            <listener>
                <ip>0.0.0.0</ip>
                <port>4002</port>
            </listener>
        </bearing>
        <obelix>
            <ip>127.0.0.1</ip>
            <port>4015</port>
            <timeout>10</timeout>
        </obelix>
        <operator>OP1</operator>
        <runway>
            <ip>127.0.0.1</ip>
            <port>34165</port>
        </runway>
        <redis timeout="6">
            <redis1>
                <ip>192.20.1.240</ip>
                <port>6379</port>
            </redis1>
            <redis2>
                <ip>127.0.0.1</ip>
                <port>6379</port>
            </redis2>
        </redis>
        <server_type>server</server_type>
        <use_route_simulation>true</use_route_simulation>
    </simulator>
    <stca>
        <listener>
            <ip>0.0.0.0</ip>
            <port>4074</port>
        </listener>
    </stca>
    <users>
        <TextSize>
            <CurTextSize>0</CurTextSize>
            <Large>12</Large>
            <Medium>10</Medium>
            <Small>8</Small>
        </TextSize>
        <antialiasing>false</antialiasing>
        <colors>
            <Act>RGBA(100,100,100,120)</Act>
            <Assume>RGBA(0,150,255,120)</Assume>
            <In>RGBA(0,0,0,0)</In>
            <SelectedText>RGBA(0,127,0,255)</SelectedText>
            <Transfered>RGBA(100,100,100,120)</Transfered>
            <gauge>RGBA(0,0,0,255)</gauge>
        </colors>
        <corridor_route>yes</corridor_route>
        <current_fdp_name>FDP</current_fdp_name>
        <current_rdp_name>Main RDP</current_rdp_name>
        <current_sector_type>ACC</current_sector_type>
        <current_user>ADMIN</current_user>
        <gauge_SI>M<!--F only feet, M only meters, FM or MF feet and meters-->
        </gauge_SI>
	<readonly_TL>false</readonly_TL>
	<readonly_QFE>false</readonly_QFE>
        <show_route_on_assume>yes</show_route_on_assume>
	<ORCAMTimeout>10</ORCAMTimeout>
    </users>
    <xt>
        <tcplistener>
            <ip>0.0.0.0</ip>
            <port>9002</port>
        </tcplistener>
    </xt>
</root>
